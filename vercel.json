{
  "buildCommand": "turbo run build",
  "outputDirectory": "apps/web/.next",
  "installCommand": "npm install",
  "ignoreCommand": "git diff --quiet HEAD^ HEAD ./apps/web/",
  "rewrites": [
    {
      "source": "/hitmaker",
      "destination": "https://useless-hitmaker.vercel.app"
    },
    {
      "source": "/hitmaker/:path*",
      "destination": "https://useless-hitmaker.vercel.app/:path*"
    }
  ],
  "trailingSlash": false,
  "cleanUrls": true
}
