{
  "rewrites": [
    { "source": "/hitmaker", "destination": "https://useless-hitmaker.vercel.app" },
    { "source": "/hitmaker/:path*", "destination": "https://useless-hitmaker.vercel.app/:path*" },
    { "source": "/hitmaker/static/:path*", "destination": "https://useless-hitmaker.vercel.app/static/:path*" }
  ],
  "trailingSlash": false,
  "cleanUrls": true,
  "routes": [
    { "handle": "filesystem" },
    { "src": "/500", "dest": "/500.html", "status": 500 },
    { "src": "/404", "dest": "/404.html", "status": 404 },
    { "src": "/hitmaker", "dest": "https://useless-hitmaker.vercel.app" },
    { "src": "/hitmaker/(.*)", "dest": "https://useless-hitmaker.vercel.app/$1" },
    { "src": "/(.*)", "status": 404, "dest": "/404.html" }
  ]
}
