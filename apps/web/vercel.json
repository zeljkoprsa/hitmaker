{
  "rewrites": [
    {
      "source": "/hitmaker",
      "destination": "https://useless-hitmaker.vercel.app"
    },
    {
      "source": "/hitmaker/:path*",
      "destination": "https://useless-hitmaker.vercel.app/:path*"
    },
    {
      "source": "/hitmaker/static/:path*",
      "destination": "https://useless-hitmaker.vercel.app/static/:path*"
    },
    {
      "source": "/404",
      "destination": "/404.html"
    },
    {
      "source": "/500",
      "destination": "/500.html"
    }
  ],
  "trailingSlash": false,
  "cleanUrls": true
}
