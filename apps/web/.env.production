# Production environment variables
# These should be configured in your deployment platform (Vercel, etc.)

# Public environment variables (exposed to browser)
NEXT_PUBLIC_APP_NAME=Useless Web
NEXT_PUBLIC_APP_VERSION=1.0.0
NEXT_PUBLIC_ENVIRONMENT=production
NEXT_PUBLIC_BASE_URL=https://tryuseless.com
NEXT_PUBLIC_HITMAKER_URL=https://tryuseless.com/hitmaker

# Prismic CMS Configuration (Public)
# Repository name is public and used for API calls
NEXT_PUBLIC_PRISMIC_ENVIRONMENT=tryuseless

# Analytics & Monitoring (Public)
NEXT_PUBLIC_ENABLE_ANALYTICS=true
NEXT_PUBLIC_ENABLE_PERFORMANCE_MONITORING=true

# Build Configuration
NEXT_TELEMETRY_DISABLED=1
GENERATE_SOURCEMAP=false

# NOTE: Private environment variables should be configured in your deployment platform
# Do NOT commit actual secrets to version control
# Examples of what should be configured in deployment:
# - PRISMIC_ACCESS_TOKEN
# - API_SECRET_KEY
# - WEBHOOK_SECRET
# - DATABASE_URL
# - EXTERNAL_API_KEY
# - EMAIL_SERVICE_API_KEY
